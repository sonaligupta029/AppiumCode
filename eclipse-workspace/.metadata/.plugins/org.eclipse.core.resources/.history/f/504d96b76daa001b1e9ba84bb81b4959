
public class Test {
	static int[] a ={12, 3, 4, 1, 6, 9}
	static int sum=24;

	public static void main(String[] args) {
		for (int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length-1;j++)
			{
				if((a[i]+a[j]+a[j+1])==sum) {
					System.out.println(a[i] +" "+a[j] +" "+a[j+1]);
//				for(int k=j+1;k<a.length;k++)
//				{
//					
//				}				
			}			
		}
	}
		

	}}

